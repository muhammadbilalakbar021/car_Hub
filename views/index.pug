script( type='text/javascript').
	$(document).ready(function(){
		console.log("Document ready");
		$('.select_Car ').on('change', function (e) {
			var el = $(this);
			console.log(el)
			 $("#results").empty();	
			var load = $('.select_Car option:selected').attr("value");
			console.log(load );
				$.ajax({
					type: 'get',
					url: "/car/"+load,
					dataType: 'json',
					success: (data) => {
						$('#carMod').html("");						
						$.each(data, function(key, value) {
						$('#carMod').append($("<option></option>")
							 .attr("value",value)
							 .text(value)); 
						});
					}
				});
			e.preventDefault(); 	
			});
	 	});

